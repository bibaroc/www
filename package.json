{
  "private": true,
  "name": "www",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build --template ./index.html",
    "serve": "sirv build --cors --single",
    "dev": "preact watch --template ./index.html",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "indent": ["error", 4, {"ignoredNodes": ["JSXAttribute"]}]
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^7.0.1",
    "eslint-config-synacor": "^3.0.5",
    "preact-cli": "^3.0.0-rc.6",
    "sirv-cli": "^0.4.5"
  },
  "dependencies": {
    "preact": "^10.1.0",
    "preact-render-to-string": "^5.1.2",
    "preact-router": "^3.2.1"
  }
}